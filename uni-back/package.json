{
  "name": "unidb-back",
  "version": "1.0.0",
  "description": "back for testing skills",
  "author": "Ilya Gamyrkin <deposha322@mail.ru>",
  "license": "ISC",
  "scripts": {
    "dev": "cd ./src && env-cmd -f ../env/.dev.env ts-node .",
    "dev-hot": "cd ./src && env-cmd -f ../.env ts-node-dev .",
    "test": "env-cmd -f ./.env.test jest --verbose --detectOpenHandles",
    "build": "tsc",
    "lint": "eslint --ext .ts,.js .",
    "start": "cd ./build && env-cmd -f ../.env node .",
    "db-create": "env-cmd sequelize db:create",
    "db-drop": "env-cmd sequelize db:drop",
    "migrate": "env-cmd sequelize db:migrate",
    "migrate-dev": "env-cmd -f ./env/.dev.env sequelize db:migrate",
    "migration-add": "env-cmd -f ./env/.dev.env sequelize migration:generate --name new",
    "migration-undo": "env-cmd -f ./env/.dev.env sequelize db:migrate:undo",
    "migration-undo-all": "env-cmd -f ./env/.dev.env sequelize db:migrate:undo:all",
    "seed": "env-cmd sequelize db:seed:all",
    "seed-dev": "env-cmd -f ./env/.dev.env sequelize db:seed:all",
    "seed-add": "env-cmd -f ./env/.dev.env sequelize seed:generate --name new",
    "seed-undo": "env-cmd -f ./env/.dev.env sequelize db:seed:undo",
    "seed-undo-all": "env-cmd -f ./env/.dev.env sequelize db:seed:undo:all",
    "migrate-in-container": "cross-env $(cat .dockerenv | xargs) sequelize db:migrate",
    "seed-in-container": "cross-env $(cat .dockerenv | xargs) sequelize db:seed:all"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/types": "^11.0.0",
    "@types/argparse": "^2.0.8",
    "@types/bcrypt": "^5.0.0",
    "@types/conventional-commits-parser": "^3.0.1",
    "@types/jest": "^26.0.24",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/koa": "^2.11.8",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-json-error": "^3.1.2",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^15.12.4",
    "@types/require-dir": "^1.0.1",
    "@types/supertest": "^2.0.11",
    "@types/url-join": "^4.0.0",
    "@types/validator": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@web-bee-ru/commitlint-config": "^1.0.1",
    "babel-jest": "^27.0.6",
    "conventional-commits-parser": "^3.2.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "4.3.8",
    "prettier": "^2.2.1",
    "request": "^2.88.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node-dev": "^1.1.1"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "bcrypt": "^5.0.1",
    "env-cmd": "^10.1.0",
    "joi": "^17.4.0",
    "joi-extract-type": "^15.0.8",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-joi-validate-middleware": "^1.1.7",
    "koa-json": "^2.0.2",
    "koa-json-error": "^3.1.2",
    "koa-jwt": "^4.0.1",
    "koa-logger": "^3.2.1",
    "koa-openapi-validator": "^4.12.0-beta.5",
    "koa-router": "^10.0.0",
    "openapi-validator-middleware": "^3.2.2",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "require-dir": "^1.2.0",
    "sequelize": "~6.6.2",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5",
    "url-join": "^4.0.1",
    "validator": "^13.5.2"
  }
}
